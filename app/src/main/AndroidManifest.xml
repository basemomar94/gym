<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gym">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="ANDROID.PERMISSION.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/gg"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Gym">
        <activity
            android:name="com.user.gym.View_Notification"
            android:exported="false"
            android:parentActivityName="com.user.gym.Notifications" />
        <activity
            android:name="com.user.gym.Plan"
            android:exported="false" />
        <activity
            android:name="com.user.gym.Profile"
            android:exported="true"
            android:parentActivityName="com.user.gym.Dashboard" />
        <activity
            android:name="com.user.gym.Subscribtion_info"
            android:exported="true"
            android:parentActivityName="com.user.gym.Dashboard" />
        <activity
            android:name="com.user.gym.MainActivity"
            android:exported="true" />
        <activity
            android:name="com.user.gym.TrainingView"
            android:exported="true"
            android:parentActivityName="com.user.gym.Training" />
        <activity
            android:name="com.user.gym.Training"
            android:exported="true"
            android:parentActivityName="com.user.gym.Dashboard" />
        <activity
            android:name="com.user.gym.Signup"
            android:exported="true" />
        <activity
            android:name="com.user.gym.Notifications"
            android:exported="true"
            android:parentActivityName="com.user.gym.Dashboard" />
        <activity
            android:name="com.user.gym.About"
            android:exported="true"
            android:parentActivityName="com.user.gym.Dashboard" />
        <activity
            android:name="com.user.gym.Dashboard"
            android:exported="true" />
        <activity
            android:name="com.user.gym.Login"
            android:exported="true" />
        <activity
            android:name="com.user.gym.Splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>